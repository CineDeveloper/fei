type Token
    value, file [UInt8]
    line, row  UInt64

func getItem(token Token) [UInt8]
    attributes
        alwaysInline
    result = token.value

func coordinatesToString(token Token) [UInt8]
    result = "File: ".join(token.file, ", line: ", (token.line + 1).toString(), ", row: ", (token.row + 1).toString(), ". ")

func replaceValue(token Token, value [UInt8]) Token
    result = token
    result:value = value
